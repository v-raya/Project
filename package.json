  "scripts": {
    "lint": "eslint --ext .jsx --ext .js app/; eslint --ext .js test/javascript/;",
    "fix-lint": "eslint --ext .jsx --ext .js --fix app/; eslint --ext .js --fix test/javascript/;exit 0;",
    "compile-webpack": "rails -g webpacker:compile",
    "dev-build": "./bin/server",
    "dev": "foreman start -f Procfile.dev;",
    "spec": "rspec spec; open reports/coverage/rspec/index.html;",
    "spec-ci": "SELENIUM_BROWSER=HEADLESS_CHROME bundle exec rspec spec;",
    "spec-ie": "SELENIUM_BROWSER=INTERNET_EXPLORER bundle exec rspec spec;",
    "karma-ci": "./bin/karma",
    "karma": "./bin/karma; open ./reports/karma_test_results.html; open ./reports/coverage/karma/index.html;",
    "karma-edge": "./bin/karma --browsers 'MSEdge - Win10'",
    "karma-ie11": "./bin/karma --browsers 'IE11 - Win7'",
    "karma-ie": "./bin/karma --browsers 'MSEdge - Win10','IE11 - Win7'",
    "all-tests": "./bin/karma;  npm run spec;",
    "spec-end2end-features": "TEST_END_TO_END=true SELENIUM_BROWSER=HEADLESS_CHROME bundle exec rspec spec/features",
    "spec-end2end-features-chrome": "TEST_END_TO_END=true SELENIUM_BROWSER=CHROME bundle exec rspec spec/features",
    "spec-end2end-features-ie": "TEST_END_TO_END=true SELENIUM_BROWSER=INTERNET_EXPLORER bundle exec rspec spec/features",
    "rebuild-assets": "rake tmp:cache:clear; rm -rf public/assets public/packs public/packs-test; bundle exec rails assets:precompile RAILS_ENV=test"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "7.x",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
